<idea-plugin url="https://github.com/AlexandreCarlton/idea-settings">
  <id>com.github.alexandrecarlton.idea.settings</id>
  <name>IDEA Settings</name>
  <vendor>Alexandre Carlton</vendor>
  <version>9999</version>

  <depends optional="true" config-file="">CheckStyle-IDEA</depends>
  <depends optional="true" config-file="">Docker</depends>
  <depends optional="true" config-file="">JavaScript</depends>
  <depends optional="true" config-file="">com.intellij.database</depends>
  <depends optional="true" config-file="">com.intellij.java</depends>
  <depends optional="true" config-file="">com.intellij.plugins.watcher</depends>
  <depends optional="true" config-file="">com.jetbrains.sh</depends>
  <depends optional="true" config-file="">com.intellij.spring.boot</depends>
  <depends optional="true" config-file="">org.jetbrains.idea.maven</depends>

  <extensions defaultExtensionNs="com.intellij">
    <appStarter implementation="com.github.alexandrecarlton.idea.settings.starter.IdeaSettingsApplicationStarter"/>
  </extensions>
</idea-plugin>
